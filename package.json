{
    "name": "rodfolk.dk-2016-design",
    "description": "Rodfolk.dk 2016 Design",
    "author": "Kamil Brego Dzielinski <brego.dk@gmail.com>",
    "version": "0.1.0",
    "license": "MIT",
    "repository": "https://github.com/brego/rodfolk.dk-2016-design",
    "paths": {
        "source": {
            "base": "front-src/",
            "html": "front-src/",
            "styles": "front-src/styles/",
            "fonts": "front-src/fonts/",
            "scripts": "front-src/scripts/",
            "images": "front-src/images/",
            "icons": "front-src/icons/"
        },
        "build": {
            "base": "public/",
            "html": "public/",
            "styles": "public/styles/",
            "fonts": "public/fonts/",
            "scripts": "public/scripts/",
            "images": "public/images/"
        }
    },
    "localConfig": {
        "autoprefixer": {
            "browsers": [
                "last 2 versions",
                "> 1%",
                "> 5% in DK",
                "iOS >= 5",
                "ie >= 8"
            ],
            "remove": false
        }
    },
    "dependencies": {
        "gulp": "3.9.0",
        "gulp-autoprefixer": "3.1.0",
        "gulp-load-plugins": "1.1.0",
        "gulp-minify-css": "1.2.1",
        "gulp-plumber": "1.0.1",
        "gulp-rename": "1.2.2",
        "gulp-replace": "0.5.4",
        "gulp-sass": "2.1.0",
        "gulp-sourcemaps": "1.6.0",
        "gulp-task-listing": "1.0.1",
        "gulp-uglify": "1.5.1",
        "mkdirp": "0.5.1",
        "node-bourbon": "4.2.3",
        "through2": "2.0.0"
    },
    "devDependencies": {
        "browser-sync": "^2.10.0",
        "del": "^1.2.1",
        "gulp-jshint": "^1.12.0",
        "gulp-scss-lint": "^0.3.8",
        "gulp-scss-lint-stylish": "^1.0.0",
        "gulp-util": "^3.0.7",
        "jshint-stylish": "^2.1.0"
    }
}
